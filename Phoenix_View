CREATE VIEW "revenue_details"("id" VARCHAR PRIMARY KEY,"details"."SERVICENAME"  VARCHAR,"details"."ZONE" VARCHAR,"details"."WARD" VARCHAR,"details"."COLLECTIONTYPE" VARCHAR,"details"."COLLDATE" VARCHAR,"details"."RCPT_AMOUNT" VARCHAR,"details"."NO_OF_RECEIPTS" VARCHAR,"details"."REFERENCEID"  VARCHAR,"details"."RECEIPTNUMBER" VARCHAR,"details"."RCPT_STATUS"  VARCHAR,"details"."GATEWAY" VARCHAR,"details"."USER_NAME" VARCHAR);
